    {
      "id": "feat-018",
      "code": "FEAT-018",
      "name": "差分升级算法",
      "epicId": "epic-009",
      "projectId": null,
      "description": "实现高效的差分升级算法，减少升级包大小",
      "productLine": "E/E架构",
      "product": "OTA平台",
      "priority": "high",
      "complexity": "high",
      "storyPoints": 44,
